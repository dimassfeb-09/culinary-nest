  "components": {
    "responses": {
      "200": {
        "description": "Status OK",
        "content": {
          "application/json": {
            "example": {
              "success": true
            }
          }
        }
      },
      "400": {
        "description": "Status Bad Request",
        "content": {
          "application/json": {
            "example": {
              "success": false,
              "message": "Payload is required."
            }
          }
        }
      },
      "404": {
        "description": "Status Not Found",
        "content": {
          "application/json": {
            "example": {
              "success": false,
              "message": "Data not found."
            }
          }
        }
      },
      "409": {
        "description": "Status Conflict",
        "content": {
          "application/json": {
            "example": {
              "success": false,
              "message": "Data already registered."
            }
          }
        }
